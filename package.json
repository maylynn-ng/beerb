{
  "name": "beerb",
  "version": "1.0.0",
  "description": "mobile app to explore London's boroughs and discover new beers",
  "main": "node_modules/expo/AppEntry.js",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "beer",
    "london",
    "social",
    "explore",
    "adventure",
    "travel",
    "map",
    "drinking"
  ],
  "author": "Maylynn Ng, Marijana Del Bianco, Bernat Duran",
  "license": "MIT",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "commitmsg": "validate-commit-msg",
    "eject": "expo eject",
    "test": "jest --collect-coverage"
  },
  "dependencies": {
    "@freakycoder/react-native-helpers": "^0.1.3",
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.6.0",
    "@types/react-redux": "^7.1.9",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "expo": "^38.0.9",
    "expo-auth-session": "^1.4.1",
    "expo-av": "~8.2.1",
    "expo-background-fetch": "~8.3.0",
    "expo-cli": "^3.28.5",
    "expo-constants": "^9.1.1",
    "expo-location": "~8.2.1",
    "expo-media-library": "~8.2.1",
    "expo-sharing": "~8.2.1",
    "expo-status-bar": "^1.0.2",
    "geolib": "^3.3.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.20",
    "lottie-react-native": "~2.6.1",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "^0.63.2",
    "react-native-dropdown-picker": "^3.1.11",
    "react-native-dynamic-search-bar": "^1.3.1",
    "react-native-dynamic-vector-icons": "^1.0.0",
    "react-native-elements": "^2.2.1",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-maps": "^0.27.1",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "~1.9.0",
    "react-native-screens": "~2.9.0",
    "react-native-share": "^3.7.0",
    "react-native-spinkit": "^1.5.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-vector-icons": "^7.0.0",
    "react-native-view-shot": "^3.1.2",
    "react-native-web": "^0.13.8",
    "react-redux": "^7.2.1",
    "react-test-renderer": "^17.0.1",
    "reanimated-bottom-sheet": "^1.0.0-alpha.22",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/react": "^16.9.46",
    "@types/react-native": "^0.63.8",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-expo": "^39.0.0",
    "jest-fetch-mock": "^3.0.3",
    "react-native-testing-library": "^6.0.0",
    "typescript": "^3.9.7",
    "validate-commit-msg": "^2.14.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "automock": false
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPatternErrorMsg": "Subject must be in format 'CMS-123 - Commit message'",
      "helpMessage": ""
    }
  }
}
